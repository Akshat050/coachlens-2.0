# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Server Configuration
PORT=8787
NODE_ENV=development

# CORS Configuration
ALLOWED_ORIGINS=chrome-extension://*,http://localhost:*

# Rate Limiting
RATE_LIMIT_POINTS=50
RATE_LIMIT_DURATION=60

# Security
HELMET_ENABLED=true

# Logging
LOG_LEVEL=info